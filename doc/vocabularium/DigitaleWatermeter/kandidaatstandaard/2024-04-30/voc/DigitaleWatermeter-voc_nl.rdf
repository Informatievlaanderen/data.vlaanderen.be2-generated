  <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#GeaggregeerdVerbruikTijdreeksobservatie">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">GeaggregeerdVerbruikTijdreeksobservatie</rdfs:label>
    <rdfs:comment xml:lang="nl">Waterverbruik op verschillende tijdstippen of periodes, geaggregeerd  over een specifiek vooraf gedefinieerde regio of groep van watermeters.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://def.isotc211.org/iso19156/2011/Observation#OM_Observation"/>
    <rdfs:subClassOf rdf:resource="http://def.isotc211.org/iso19156/2011/TimeSeriesObservation#OM_TimeSeriesObservation"/>
  </owl:Class>
  <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#IndividueelVerbruikObservatie">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">IndividueelVerbruikObservatie</rdfs:label>
    <rdfs:comment xml:lang="nl">Individueel waterverbruik op één aansluitingspunt gerelateerd aan één meter.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://def.isotc211.org/iso19156/2011/Observation#OM_Observation"/>
    <rdfs:subClassOf rdf:resource="http://def.isotc211.org/iso19156/2011/Measurement#OM_Measurement"/>
  </owl:Class>
  <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#IndividueelVerbruikTijdreeksobservatie">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">IndividueelVerbruikTijdreeksobservatie</rdfs:label>
    <rdfs:comment xml:lang="nl">Het waterverbruik gemeten of voorspeld op één aansluitingspunt gerelateerd aan meerdere tijdstippen (index) of periodes (volume).</rdfs:comment>
    <ns1:usageNote xml:lang="nl">bv. voorspelling van waterverbruik voor de komende 7 dagen.</ns1:usageNote>
    <rdfs:subClassOf rdf:resource="http://def.isotc211.org/iso19156/2011/TimeSeriesObservation#OM_TimeSeriesObservation"/>
    <rdfs:subClassOf rdf:resource="http://def.isotc211.org/iso19156/2011/Observation#OM_Observation"/>
  </owl:Class>
  <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#MeterstaatObservatie">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">MeterstaatObservatie</rdfs:label>
    <rdfs:comment xml:lang="nl">Observatie gerelateerd aan de staat van het metertoestel.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Bijvoorbeeld batterijstatus of tijdstip laatste contact.</ns1:usageNote>
    <rdfs:subClassOf rdf:resource="http://def.isotc211.org/iso19156/2011/Observation#OM_Observation"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#RuimtelijkRelatie.plaats.source">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">Plaats (source)</rdfs:label>
    <rdfs:comment xml:lang="nl">Referentie naar verbonden klasse.</rdfs:comment>
    <rdfs:domain>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#RuimtelijkRelatie">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
        <rdfs:label xml:lang="nl">RuimtelijkRelatie</rdfs:label>
        <rdfs:comment xml:lang="nl">Onderling verband tussen twee Plaatsen.</rdfs:comment>
        <ns1:usageNote xml:lang="nl">Zo kan een bevoorradingsgebied ook als Meterzone worden geïnterpreteerd of kan men zones groeperen tot superzone.</ns1:usageNote>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/Location"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#RuimtelijkRelatie.plaats.target">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">Plaats (target)</rdfs:label>
    <rdfs:comment xml:lang="nl">Referentie naar verbonden klasse.</rdfs:comment>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#RuimtelijkRelatie"/>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/Location"/>
  </owl:ObjectProperty>
  <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#Software">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">Software</rdfs:label>
    <rdfs:comment xml:lang="nl">The set of instructions that indicate a machine how to work. In this ontology software is a general concept which acts as a superclass for software versions, docker images, data transformations, etc.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://www.w3.org/ns/sosa/Sensor"/>
  </owl:Class>
  <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#Verbruikszone">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">Verbruikszone</rdfs:label>
    <rdfs:comment xml:lang="nl">Zone binnen het bevoorradingsgebied bestaande uit vaste registreerzones bestaande uit vaste registreerzones waarbinnen de in- en uitgaande waterstromen permanent bemeterd worden.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/dc/terms/Location"/>
  </owl:Class>
  <owl:DatatypeProperty rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#aantalMeters">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">aantalMeters</rdfs:label>
    <rdfs:comment xml:lang="nl">Het aantal meters dat binnen de Meterzone ligt.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Dit attribuut kan gebruikt worden om zones samen te voegen (mbt GDPR) indien er zich te weinig meters binnen één zone bevinden.</ns1:usageNote>
    <rdfs:domain>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#Meterzone">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
        <rdfs:label xml:lang="nl">Meterzone</rdfs:label>
        <rdfs:comment xml:lang="nl">Afgebakend geografisch gebied waarover geaggregeerd statistiek wordt berekend waarbinnen één of meerdere verbruiksobservatie(s) vallen</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://purl.org/dc/terms/Location"/>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#afleverpunt">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">afleverpunt</rdfs:label>
    <rdfs:comment xml:lang="nl">Aard van de aansluiting waar een voorziening (electriciteit, water, ...) of dienst geleverd wordt. Gaat het om een gebouw of gebouweenheid?</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Gebouweenheid is 1:1 gelinkt aan een meter of verbruiksplaats van de dienst die geleverd wordt en dus aan een individuele observatie. Een gebouw kan gelinkt zijn aan meerdere gebouweenheden, bv bij een appartementsgebouw.</ns1:usageNote>
    <rdfs:domain rdf:resource="https://vocab.belgif.be/ns/utility-services#Connection"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#element">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
    <rdfs:label xml:lang="nl">element</rdfs:label>
    <rdfs:comment xml:lang="nl">Een element dat gegevens vertegenwoordigt van een specifiek tijdstip of periode.</rdfs:comment>
    <rdfs:domain>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#TemporeelMomentKwantitatiefBereik">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
        <rdfs:label xml:lang="nl">TemporeelMomentKwantitatiefBereik</rdfs:label>
        <rdfs:comment xml:lang="nl">Resultaat van de observatie in de vorm van een tijdreeks.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://def.isotc211.org/iso19156/2011/TemporalCoverage#CVT_DiscreteTimeInstantCoverage"/>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#MomentKwantitatieveWaardepaar">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters"/>
        <rdfs:label xml:lang="nl">MomentKwantitatieveWaardepaar</rdfs:label>
        <rdfs:comment xml:lang="nl">Een kwantitatief waardepaar dat onderdeel is van een tijdreeks</rdfs:comment>
        <ns1:usageNote xml:lang="nl">Bestaat uit een tijdstip en een waarde voor één of meerdree punten in de tijd. Het kan bijvoorbeeld gebruikt worden om een voorspelling van waterverbruik te doen voor verschillende dagen.</ns1:usageNote>
        <rdfs:subClassOf rdf:resource="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#MomentWaardepaar"/>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/nutsvoorzieningen/meters#hoofdaansluiting">
