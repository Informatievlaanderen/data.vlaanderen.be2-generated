  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BeslissingTotBestuurlijkOpsporingsonderzoek",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Beslissing tot Bestuurlijk Opsporingsonderzoek"
      },
      "definition": {
        "nl": "Beslissing van de Vervolgingsinstantie om een bestuurlijk opsporingsonderzoek op te starten, i.e. het geheel van de handelingen die ertoe strekken bestuurlijk sanctioneerbare feiten, hun daders en de bewijzen ervan op te sporen en gegevens te verzamelen die dienstig zijn voor de bestuurlijke vervolging."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeBeslissing"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BeslissingTotBestuurlijkSepot",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Beslissing tot Bestuurlijk Sepot"
      },
      "definition": {
        "nl": "Beslissing van de Vervolgingsinstantie om over te gaan tot bestuurlijke sepot, i.e. de beslissing om een sanctioneerbaar feit niet te vervolgen."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeBeslissing"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BeslissingTotBestuurlijkeVervolging",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Beslissing tot Bestuurlijke Vervolging"
      },
      "definition": {
        "nl": "Beslissing om over te gaan tot bestuurlijke vervolging."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeBeslissing"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeBeslissing",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Bestuurlijke Beslissing"
      },
      "definition": {
        "nl": "Beslissing genomen door de bevoegden."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeGeldboete",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Bestuurlijke Geldboete"
      },
      "definition": {
        "nl": "Geldbedrag dat betaald moet worden naar aanleiding van een overtreding."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeSanctie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeSanctie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Bestuurlijke Sanctie"
      },
      "definition": {
        "nl": "Iets dat opgelegd wordt ter sanctionering van bepaald ongewenst gedrag en dat gericht is op leedtoevoeging."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#Feit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Feit"
      },
      "definition": {
        "nl": "De handelingen of nalatigheden van de betrokkene."
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#OnmiddellijkeInning",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Onmiddellijke Inning"
      },
      "definition": {
        "nl": "Het onmiddellijk betalen van een geldbedrag in handen van een toezichthouder/opsporingsagent waardoor de betrokkene niet wordt vervolgd."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeBeslissing"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#SanctieBeslissing",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Sanctiebeslissing"
      },
      "definition": {
        "nl": "Beslissing van de ambtenaren of de entiteiten van de Vlaamse overheid die hetzij binnen eenzelfde beleidsdomein, hetzij voor eenzelfde regelgeving door de Vlaamse Regering belast zijn met het opleggen van bestuurlijke sancties voor bepaalde misdrijven of inbreuken."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeBeslissing"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#UitsluitingVanSteun",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Uitsluiting van Steun"
      },
      "definition": {
        "nl": "Aanvullende Sanctie waarbij men het niet toestaat dat steun verkregen wordt."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeSanctie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#Verbeurdverklaring",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Verbeurdverklaring"
      },
      "definition": {
        "nl": "Aanvullende Sanctie waarbij de betrokkene de eigendom over een bepaald vermogensbestanddeel verliest en waarbij de staat die eigendom verkrijgt."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#BestuurlijkeSanctie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijk-sanctieregister#VervalSanctiemogelijkheidDoorBetalingGeldsom",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Verval van de Sanctiemogelijkheid door Betaling van een Geldsom"
      },
      "definition": {
        "nl": "Tijdige betaling van een geldsom door de betrokkene op voorstel van de Vervolgingsinstantie. Hierdoor vervalt de mogelijkheid om over te gaan tot een Bestuurlijke Sanctie."
      },
      "usage": {
        "nl": "Ook gekend onder de termen \"voorstel tot betaling van een geldsom\" en \"bestuurlijke transactie\"."
      },
      "parents": []
    }
  ],
