  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#AanvullendeSanctie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Bijkomende Bestuurlijke Sanctie die (facultatief) bovenop de Hoofdsanctie wordt opgelegd."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BestuurlijkeSanctie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#AlternatieveSanctie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Bestuurlijke Sanctie als gevolg van het doorlopen van de alternatieve, bestuurlijke vervolgingsprocedure die voortvloeit uit een Beslissing tot Alternatieve Procedure van de vervolgingsinstantie."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BestuurlijkeSanctie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingBeboetingsinstantie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Beslissing van de ambtenaren of de entiteiten van de Vlaamse overheid die hetzij binnen eenzelfde beleidsdomein, hetzij voor eenzelfde regelgeving door de Vlaamse Regering belast zijn met het opleggen van bestuurlijke sancties voor bepaalde misdrijven of inbreuken."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BestuurlijkeBeslissing"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingToezichthouder",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Beslissing van een Organisatie die bevoegd is voor het uitoefenen van bestuurlijk toezicht."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BestuurlijkeBeslissing"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingTotAlternatieveProcedure",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Beslissing van de Vervolgingsinstantie om de alternatieve, bestuurlijke sanctieprocedure op te starten."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingTotBestuurlijkeVervolging"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingTotBestuurlijkOpsporingsonderzoek",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Beslissing van de Vervolgingsinstantie om een bestuurlijk opsporingsonderzoek op te starten, i.e. het geheel van de handelingen die ertoe strekken bestuurlijk sanctioneerbare feiten, hun daders en de bewijzen ervan op te sporen en gegevens te verzamelen die dienstig zijn voor de bestuurlijke vervolging."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingVervolgingsinstantie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingTotBestuurlijkSepot",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Beslissing van de Vervolgingsinstantie om over te gaan tot bestuurlijke sepot, i.e. de beslissing om een sanctioneerbaar feit niet te vervolgen."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingVervolgingsinstantie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingTotBestuurlijkeVervolging",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Beslissing van de Vervolgingsinstantie om over te gaan tot bestuurlijke vervolging."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingVervolgingsinstantie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingVervolgingsinstantie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Beslissing van de ambtenaren of de entiteiten van de Vlaamse overheid die hetzij binnen eenzelfde beleidsdomein, hetzij voor eenzelfde regelgeving door de Vlaamse Regering belast zijn met de bestuurlijke vervolging van bepaalde misdrijven of inbreuken."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BestuurlijkeBeslissing"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BestuurlijkeBeslissing",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Beslissing genomen door een bestuur."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/besluitvorming#Beslissingsactiviteit"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BestuurlijkeGeldboete",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Geldbedrag dat betaald moet worden naar aanleiding van een overtreding."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#Hoofdsanctie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BestuurlijkeSanctie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Iets dat opgelegd wordt ter sanctionering van bepaald ongewenst gedrag en dat gericht is op leedtoevoeging."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#GepleegdFeit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "De handelingen of nalatigheden van de betrokkene."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#Hoofdsanctie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "De primaire Bestuurlijke Sanctie."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BestuurlijkeSanctie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#OnmiddellijkeInning",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Het onmiddellijk betalen van een geldbedrag in handen van een toezichthouder/opsporingsagent waardoor de betrokkene niet wordt vervolgd."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#BeslissingToezichthouder"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#UitsluitingVanSteun",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Aanvullende Sanctie waarbij men het niet toestaat dat steun verkregen wordt."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#AanvullendeSanctie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#Verbeurdverklaring",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Aanvullende Sanctie waarbij de betrokkene de eigendom over een bepaald vermogensbestanddeel verliest en waarbij de staat die eigendom verkrijgt."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/bestuurlijke-sancties#AanvullendeSanctie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/bestuurlijke-sancties#VervalSanctiemogelijkheidDoorBetalingGeldsom",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Tijdige betaling van een geldsom door de betrokkene op voorstel van de Vervolgingsinstantie. Hierdoor vervalt de mogelijkheid om over te gaan tot een Bestuurlijke Sanctie."
      },
      "usage": {
        "nl": "Ook gekend onder de termen \"voorstel tot betaling van een geldsom\" en \"bestuurlijke transactie\"."
      },
      "parents": []
    }
  ],
