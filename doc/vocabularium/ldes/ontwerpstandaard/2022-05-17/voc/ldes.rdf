  <owl:Class rdf:about="https://data.vlaanderen.be/ns/ldes#DataServiceLDES">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">Een verzameling van operaties die ofwel toegang bieden tot één of meer Linked Data Event Streams.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Het protocol is hier per default dat van de Linked Data Event Stream.</ns1:usageNote>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#DataService"/>
  </owl:Class>
  <owl:Class rdf:about="https://data.vlaanderen.be/ns/ldes#OnveranderlijkObject">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">Een onveranderlijkObject is het basiselement van een Linked Data Event Stream.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Een onveranderlijk object is het basiselement van een eventstream.</ns1:usageNote>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/ldes#CollectieLid">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
        <rdfs:comment xml:lang="nl">Een object dat deel uitmaakt van een collectie.</rdfs:comment>
        <ns1:usageNote xml:lang="nl">Kan verder gespecialiseerd worden in functie van het soort collectie, bijvoorbeeld als de collectie een event stream is zijn de collectieleden onveranderlijke objecten.</ns1:usageNote>
      </owl:Class>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://data.vlaanderen.be/ns/ldes#VeranderlijkObject">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">Een veranderlijk object is een object wiens status nog kan veranderen nadat het werd gecreëerd.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Een veranderlijk object wordt gebruikt als basiselement van een dataset. </ns1:usageNote>
    <rdfs:subClassOf rdf:resource="https://data.vlaanderen.be/ns/ldes#CollectieLid"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/ldes#eventstreamView">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">De event stream view connecteert de collectie met de rootnode zodanig dat alle leden van de collectie gevonden kunnen worden.</rdfs:comment>
    <rdfs:domain>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/ldes#RootNode">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
        <rdfs:comment xml:lang="nl">Een RootNode is een beginpagina, die zelf al leden kan bevatten, via wiens gelinkte pagina's alle leden kunnen gevonden worden.</rdfs:comment>
        <ns1:usageNote xml:lang="nl">Naar de rootnode wordt verwezen vanuit de eventstream bij voorkeur via een eventstream-view.</ns1:usageNote>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/ldes#EventstreamView">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
        <rdfs:comment xml:lang="nl">De event stream view connecteert de collectie met de rootnode zodanig dat alle leden van de collectie gevonden kunnen worden.</rdfs:comment>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/ldes#retentiebeleid">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">Een retentiebeleid is een beleid volgens welke elementen (i.e. data) verwijderd zullen worden.</rdfs:comment>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/ldes#EventstreamView"/>
    <rdfs:range rdf:resource="https://w3id.org/ldes#RetentionPolicy"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/ldes#rootNode">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">Een rootnode is een beginpagina, die zelf al leden kan bevatten, via wiens gelinkte pagina’s alle leden kunnen gevonden worden.</rdfs:comment>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/ldes#EventstreamView"/>
    <rdfs:range rdf:resource="https://data.vlaanderen.be/ns/ldes#RootNode"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://data.vlaanderen.be/ns/ldes#snapshotTot">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">Het tijdstip waarop de snapshot van de Linked Data Event Stream werd genomen. </rdfs:comment>
    <rdfs:domain>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/ldes#Snapshot">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
        <rdfs:comment xml:lang="nl">Een event stream van versies waarbij enkel laatste versie behouden blijft t.o.v. een opgegeven datum.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="https://w3id.org/ldes#EventStream"/>
        <rdfs:subClassOf rdf:resource="https://www.w3.org/ns/dcat#Dataset"/>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/ldes#snapshotVan">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">Verwijst naar de originele Linked Data Event Stream.</rdfs:comment>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/ldes#Snapshot"/>
    <rdfs:range>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/ldes#VersieEventStream">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
        <rdfs:comment xml:lang="nl">Een event stream waarvan de leden versies zijn.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="https://w3id.org/ldes#EventStream"/>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://data.vlaanderen.be/ns/ldes#versieMaterialisatieTot">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">Het tijdstip tot wanneer versies werden verwerkt.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Het attribuut versieMaterialisatieTot geeft weer tot op welk moment de versies werden geaggregeerd.</ns1:usageNote>
    <rdfs:domain>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/ldes#VersieMaterialisatie">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
        <rdfs:comment xml:lang="nl">Een snapshot waarbij de versie-identificatoren zijn vervangen door object-identificatoren.</rdfs:comment>
        <ns1:usageNote xml:lang="nl">Om deze reden (zie definitie) zijn de collectieleden veranderlijke - in plaats van onveranderlijke objecten.</ns1:usageNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/tree#Collection"/>
        <rdfs:subClassOf rdf:resource="https://www.w3.org/ns/dcat#Dataset"/>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/ldes#versiematerialisatieVan">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/ldes"/>
    <rdfs:comment xml:lang="nl">Verwijst naar de originele Linked Data Event Stream.</rdfs:comment>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/ldes#VersieMaterialisatie"/>
    <rdfs:range rdf:resource="https://data.vlaanderen.be/ns/ldes#Snapshot"/>
  </owl:ObjectProperty>
</rdf:RDF>
