  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/loongegevens#afTeTrekkenNettoBedragen">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/loongegevens"/>
    <rdfs:label xml:lang="nl">afTeTrekkenNettoBedragen</rdfs:label>
    <rdfs:comment xml:lang="nl">Verplichte inhoudingen door de werkgever.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Dit gaat over de sommen voor loonbeslag, loonoverdracht, persoonlijke bijdrage maaltijdcheques, ...</ns1:usageNote>
    <rdfs:domain>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/loongegevens#Arbeidsloon">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/loongegevens"/>
        <rdfs:label xml:lang="nl">Arbeidsloon</rdfs:label>
        <rdfs:comment xml:lang="nl">Het bedrag dat een persoon ontvangt voor geleverde prestaties in arbeidsverband. Het kan gaan om een financieel bedrag, een bedrag in natura, vergoedingen, etc.</rdfs:comment>
        <rdfs:subClassOf>
          <owl:Class rdf:about="https://data.vlaanderen.be/ns/loongegevens#Inkomst">
            <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/loongegevens"/>
            <rdfs:label xml:lang="nl">Inkomst</rdfs:label>
            <rdfs:comment xml:lang="nl">Het bedrag dat een persoon ontvangt voor geleverde prestaties, beschikbaar gestelde goederen, land of vermogen.</rdfs:comment>
          </owl:Class>
        </rdfs:subClassOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="https://schema.org/MonetaryAmount"/>
  </owl:ObjectProperty>
