<rdfs:isDefinedBy rdf:resource='https://data.vlaanderen.be/ns/openbaardomein/infrastructuur' />

<rdfs:subClassOf>
<owl:Class rdf:about='https://data.vlaanderen.be/ns/openbaardomein/infrastructuur#Infrastructuurelement'>
