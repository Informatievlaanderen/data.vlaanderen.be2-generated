  rdfs:comment "Het aantal assen waar het voertuig op rust."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/vrachtwagenparkeren#Voertuig>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/vrachtwagenparkeren>;
