  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#Agens",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een chemisch element of chemische verbinding (in zuivere vorm of in een mengsel) zoals deze in natuurlijke staat voorkomt of het resultaat is van, gebruikt of vrijgekomen is bij een beroepsactiviteit, al dan niet opzettelijk geproduceerd."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#ChemischAgensVrachtObservatie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Observatie om de chemische kwaliteit van water te beoordelen aan de hand van de afgemeten hoeveelheid vracht."
      },
      "parents": [
        "http://def.isotc211.org/iso19156/2011/Observation#OM_Observation",
        "http://def.isotc211.org/iso19156/2011/Measurement#OM_Measurement"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#Emissie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Uitstoot van iets."
      },
      "usage": {
        "nl": "Typisch productie en afvoer van een verontreinigende stof, bv. een gas in de lucht of een vloeistof in water. Kan bv. ook geluid of straling zijn."
      },
      "parents": [
        "http://def.isotc211.org/iso19156/2011/GeneralFeatureInstance#GFI_Feature"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#Emissiebron",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Oorsprong van een emissie."
      },
      "parents": [
        "http://def.isotc211.org/iso19156/2011/GeneralFeatureInstance#GFI_Feature"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#GeobserveerdWeer",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een observatie van weersomstandigheden op een zekere plaats en tijd."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#HydromorfologischeIndexObservatie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "parents": [
        "http://def.isotc211.org/iso19156/2011/Observation#OM_Observation",
        "http://def.isotc211.org/iso19156/2011/Measurement#OM_Measurement",
        "https://data.vlaanderen.be/ns/waterkwaliteit#StatistischeObservatie"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#Meetnet",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Samenhangend geheel van plaatsen waar Observaties worden uitgevoerd."
      },
      "parents": [
        "http://def.isotc211.org/iso19156/2011/SamplingFeature#SF_SamplingFeatureCollection"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#Meetpunt",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Plaats waar Observaties plaatsvinden."
      },
      "parents": [
        "http://def.isotc211.org/iso19156/2011/SpatialSamplingFeature#SF_SpatialSamplingFeature"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#Soort",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een levend organisme zoals planten, dieren en microben die gebruikt worden om de gezondheid van het natuurlijk ecosysteem in kaart te brengen."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#StatistischeObservatie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een observatie volgens een statistische methodologie."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/waterkwaliteit#WaterkwaliteitObservatieVerzameling",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een verzameling van één of meerdere waterkwaliteitobservaties. De geïncludeerde observaties delen een waarde voor één of meerdere eigenschappen."
      },
      "parents": [
        "https://www.w3.org/TR/vocab-ssn-ext/#sosa:ObservationCollection"
      ]
    }
  ],
