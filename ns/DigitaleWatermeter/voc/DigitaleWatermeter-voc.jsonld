      "label": {
        "nl": "aantalMeters"
      },
      "definition": {
        "nl": "Het aantal meters dat binnen de Meterzone ligt."
      },
      "usage": {
        "nl": "Dit attribuut kan gebruikt worden om zones samen te voegen (mbt GDPR) indien er zich te weinig meters binnen één zone bevinden."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/DigitaleWatermeter#Meterzone"
      ],
      "range": [
        "http://www.w3.org/2001/XMLSchema#integer"
