      <owl:Class rdf:about="https://data.vlaanderen.be/ns/grondboringen#Grondboring">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/grondboringen"/>
        <rdfs:label xml:lang="nl">Grondboring</rdfs:label>
        <rdfs:comment xml:lang="nl">De handeling van het boren van een smalle schacht in de grond.</rdfs:comment>
        <ns1:usageNote xml:lang="nl">Onder "grond" verstaan we hier het deel van de aardkorst onder het aardoppervlak. Het slaat zowel op de bodem als op wat zich daaronder bevindt (en hier ondergrond wordt genoemd).</ns1:usageNote>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Sampling"/>
        <rdfs:subClassOf rdf:resource="https://data.vlaanderen.be/ns/bodem-en-ondergrond#Boring"/>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/Agent"/>
    <rdfs:subPropertyOf rdf:resource="https://data.vlaanderen.be/ns/sensoren-en-bemonstering#Bemonstering.uitgevoerdDoor"/>
  </owl:ObjectProperty>
