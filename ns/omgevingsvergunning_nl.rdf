    <rdfs:label xml:lang="nl">ingangsdatum</rdfs:label>
    <ns1:usageNote xml:lang="nl">Bijvoorbeeld, de datum van een arrest.</ns1:usageNote>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning#Rechtshandeling"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/omgevingsvergunning#inhoud">
