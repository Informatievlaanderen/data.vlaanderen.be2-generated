    <rdfs:label xml:lang="nl">inhoud</rdfs:label>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning#Rechtshandeling"/>
    <rdfs:range rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning#Inhoud"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/omgevingsvergunning#juridischeKwalificatie">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning"/>
    <rdfs:label xml:lang="nl">juridische kwalificatie</rdfs:label>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning#Rechtshandeling"/>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning#Activiteit"/>
    <rdfs:range>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/omgevingsvergunning#JuridischeKwalificatie">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning"/>
        <rdfs:label xml:lang="nl">Juridische Kwalificatie</rdfs:label>
        <rdfs:comment xml:lang="nl">Kwalificatie van de aard van de activiteit in termen van juridisch vastgelegde typeringen.</rdfs:comment>
        <ns1:usageNote xml:lang="nl">Onder Juridische Categorie verstaan we rubrieken, klasseringen of juridisch-verankerde typeringen. De kwalificatie gebeurt aan de hand van Juridische Verwijzingen, en meer bepaald verwijzingen naar Juridische Werken of Juridische Categorieën. Het attribuut 'kwalificatietype' laat toe om een juridische kwalificatie op verschillende dimensies (bv. een activiteit kwalificeren op vlak van indelingsrubrieken en op vlak van Sevezo-regelgeving) te doen.</ns1:usageNote>
      </owl:Class>
    </rdfs:range>
    <rdfs:comment xml:lang="nl">De Juridische Kwalificatie van de Activiteit.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/omgevingsvergunning#kwalificatietype">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning"/>
    <rdfs:label xml:lang="nl">kwalificatietype</rdfs:label>
    <rdfs:comment xml:lang="nl">De type van de Juridische Kwalificatie.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Een Juridische Kwalificatie kan gebeuren op verschillende dimensies. Het voorwerp van een omgevingsaanvraag kan gekwalificeerd worden op vlak van IIOA én Seveso regelgeving.</ns1:usageNote>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning#JuridischeKwalificatie"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/omgevingsvergunning#kwalificatiewaarde">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning"/>
    <rdfs:label xml:lang="nl">kwalificatiewaarde</rdfs:label>
    <rdfs:comment xml:lang="nl">Verwijzingen naar Juridische Werken die als ankerpunten voor de kwalificatie dienen.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Doorgaans een Juridisch Werk, zoals een wet of decreet, of een Juridische Categorie.</ns1:usageNote>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/omgevingsvergunning#JuridischeKwalificatie"/>
    <rdfs:range rdf:resource="http://fixme.com#JuridischWerk"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/omgevingsvergunning#locatie">
