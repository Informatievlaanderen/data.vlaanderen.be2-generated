  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/schuldbeheer#aanleiding">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/schuldbeheer"/>
    <rdfs:label xml:lang="nl">aanleiding</rdfs:label>
    <rdfs:comment xml:lang="nl">Verwijzing naar de aanleiding voor de schuldvordering.</rdfs:comment>
    <rdfs:domain>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/schuldbeheer#Schuldvordering">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/schuldbeheer"/>
        <rdfs:label xml:lang="nl">Schuldvordering</rdfs:label>
        <rdfs:comment xml:lang="nl">Aanspraak van een partij (de schuldeiser) op een bepaalde prestatie vanwege een andere partij (de schuldenaar).</rdfs:comment>
        <ns1:usageNote xml:lang="nl">Voor de doeleinden van dit applicatieprofiel is de opgelegde prestatie altijd de betaling van een geldsom. </ns1:usageNote>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/schuldbeheer#aanmaning">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/schuldbeheer"/>
    <rdfs:label xml:lang="nl">aanmaning</rdfs:label>
    <rdfs:comment xml:lang="nl">Verwijzing naar een aanmaning  met betrekking tot de schuldvordering.</rdfs:comment>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/schuldbeheer#Schuldvordering"/>
    <rdfs:range>
      <owl:Class rdf:about="https://data.vlaanderen.be/ns/schuldbeheer#Aanmaning">
        <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/schuldbeheer"/>
        <rdfs:label xml:lang="nl">Aanmaning</rdfs:label>
        <rdfs:comment xml:lang="nl">Schriftelijk berichtgeving van een schuldeiser naar een schuldenaar om deze schuldenaar te herinneren aan een schuldvordering die niet is voldaan terwijl de uiterste betaaldatum van deze schuldvordering reeds verstreken is.  </rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
