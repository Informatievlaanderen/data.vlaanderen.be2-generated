  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#DiscreteProbabiliteit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Statistiek die beschrijft hoe waarschijnlijk het is dat een variable binnen een bepaalde categorie valt."
      },
      "usage": {
        "nl": "Bvb de kans dat de begroeiing uit eik bestaat."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Drempelwaarde",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Waarde die aangeeft wanneer een grens wordt overschreden."
      },
      "usage": {
        "nl": "Bvb 10m voor een maximaal waterpeil."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Kwantiel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Statistiek die betrekking heeft op een bepaald kwantiel."
      },
      "usage": {
        "nl": "Onder kwantiel verstaan we de subset van data die ontstaat door de volledige set te ordenen en in gelijke aantallen op te delen. Bvb een kwartiel van een set waarnemingen bevat 25% van alle waarnemingen. De Statistiek is dan bvb het gemiddelde van alle waarden in het eerste kwartiel."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Moment",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Statistiek die aangeeft hoe de waarden gespreid zijn tov een centrale maat."
      },
      "usage": {
        "nl": "Bvb de spreiding tov het gemiddelde, uitgedrukt als de standaarddeviatie. De Statistiek is bvb de standaardddeviatie tov het gemiddelde."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Probabiliteit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Statistiek die beschrijft hoe waarschijnlijk het is dat een variabele een bepaalde drempelwaarde (al dan niet) overschrijdt."
      },
      "usage": {
        "nl": "Bvb de kans dat het waterpeil 10m overschrijdt."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Statistiek",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "nl": "Een stuk informatie dat een kenmerk van een dataset of variable beschrijft."
      },
      "parents": []
    }
  ],
