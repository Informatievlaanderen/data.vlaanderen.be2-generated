    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#niveau",
      "@type": "http://www.w3.org/2002/07/owl#DatatypeProperty",
      "label": {
        "nl": "niveau"
      },
      "definition": {
        "nl": "Nummer van het kwantiel waarvoor de Statistiek geldt."
      },
      "usage": {
        "nl": "Bvb 1 als de Statistiek betrekking heeft op het eerste kwartiel."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/statistiek#Kwantiel"
