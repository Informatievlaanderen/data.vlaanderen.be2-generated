    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#operator",
      "@type": "http://www.w3.org/2002/07/owl#ObjectProperty",
      "label": {
        "nl": "operator"
      },
      "definition": {
        "nl": "Vergelijkingsoperator."
      },
      "usage": {
        "nl": "In voornoemd vb zou de drempel overschreden worden bij waarden 'groter of gelijk aan' 10m."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/statistiek#Drempelwaarde"
      ],
