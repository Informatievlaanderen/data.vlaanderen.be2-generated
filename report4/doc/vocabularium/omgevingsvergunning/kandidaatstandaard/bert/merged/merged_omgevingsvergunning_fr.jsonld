      "vocDefinition": [
        {
          "@language": "nl",
          "@value": "Aanspraak op de uitoefening van een bevoegdheid; formeel verzoek van een rechtssubject aan een bevoegde instantie om informatie te verstrekken of een beslissing te nemen, in het bijzonder een beslissing om ergens toelating voor te verlenen."
        },
        {
          "@language": "fr",
          "@value": "Droit à l’exercice d’un pouvoir ; demande formelle d’une personne morale à une autorité compétente de fournir des informations ou de prendre une décision, notamment une décision d’autoriser quelque chose."
        }
      ],
      "vocLabel": [
        {
          "@language": "nl",
          "@value": "Aanvraag"
        },
        {
          "@language": "fr",
          "@value": "Demander"
        }
      ],
      "vocUsageNote": [
        {
          "@language": "nl",
          "@value": "In de context van omgevingsvergunningen betreft dit alle aanvragen van of met betrekking tot omgevingsvergunningen."
        },
        {
          "@language": "fr",
          "@value": "Dans le cadre des permis d’environnement, il s’agit de toutes les demandes de permis d’environnement ou relatives à ceux-ci."
        }
      ]
    },
